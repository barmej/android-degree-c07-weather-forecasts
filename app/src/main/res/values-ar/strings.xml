    <string name="title_activity_settings">الإعدادات</string>

    <!-- Used to open Settings Menu from MainActivity and DetailActivity -->
    <string name="action_settings">الإعدادات</string>

    <!-- - - - - - - - - - - - - - - - -
     - Secondary weather info labels   -
     - - - - - - - - - - - - - - - - - -->
    <string name="wind_label">الرياح</string>
    <string name="pressure_label">الضغط</string>
    <string name="humidity_label">الرطوبة</string>

    <!-- - - - - - - - - - - - - - - - -
      - Preference Labels              -
      - - - - - - - - - - - - - - - - - -->
    <string name="pref_location_label">الموقع</string>
    <string name="pref_units_label">نظام وحدات القياس</string>
    <string name="pref_units_label_metric">النظام المتري</string>
    <string name="pref_units_label_imperial">النظام الامبراطوري</string>

    <!-- - - - - - - - - - - - - - - - -
      - Used by WeatherUtils           -
      - - - - - - - - - - - - - - - - -->

    <!-- HighLow Temperature format -->
    <string name="high_low_temperature"> \u200F
        \u00B0<xliff:g id="high_temp">%1$1.0f</xliff:g> /
        \u00B0<xliff:g id="low_temp">%2$1.0f</xliff:g>
    </string>

    <!-- Temperature format -->
    <string name="format_temperature">
        <xliff:g id="temp">%1.0f</xliff:g>\u00B0
    </string>

    <!-- Wind in kph -->
    <string name="format_wind_kmh">
        <xliff:g id="speed">%1$1.0f</xliff:g> كم/س
        <xliff:g id="direction">%2$s</xliff:g>
    </string>

    <!-- Wind in mph -->
    <string name="format_wind_mph">
        <xliff:g id="speed">%1$1.0f</xliff:g> م/س
        <xliff:g id="direction">%2$s</xliff:g>
    </string>

    <!-- Pressure format -->
    <string name="format_pressure">
        <xliff:g id="pressure">%1.0f</xliff:g> هكتوبسكال
    </string>

    <!-- Humidity format -->
    <string name="format_humidity">
        <xliff:g id="humidity">%1.0f</xliff:g> %%
    </string>

    <!-- - - - - - - - - - - - - - - - -
      - Wind Directions                -
      - - - - - - - - - - - - - - - - -->
    <string name="unknown_direction">غير معروف</string>
